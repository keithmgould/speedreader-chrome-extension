{
  "manifest_version": 2,
  "name": "Speed Reader",
  "description": "Highlight text and read it through a speed-reader widget",
  "version": "0.1",
  "browser_action": {
    "default_icon" : "eye.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/selection.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}
